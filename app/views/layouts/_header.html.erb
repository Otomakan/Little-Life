<% if logged_in? %>
<% greet = "Hi! " + current_user.name + "!" %>
<% userLink = '/users/'+current_user.id.to_s %>
<nav>
      <div class="nav-fostrap">
        <ul>
          <li><%= link_to greet, '/'%></li>
          <li><%= link_to "Dashboard", userLink%></li>
          <li><a href="javascript:void(0)" >Navigation<span class="arrow-down"></span></a>
            <ul class="dropdown">
              <li><%= link_to "About", '/about' %></li>
              <li><%= link_to "See all users", '/test' %></li>
              <li><%= link_to "Help", '/help' %></li>
              <li><%= link_to "Home", '/' %></li>
            </ul>
          </li>
        
          <li><%= link_to "Log out",  logout_path, method: :delete %></li>
        </ul>
      </div>
      <div class="nav-bg-fostrap">
        <div class="navbar-fostrap"> <span></span> <span></span> <span></span> </div>
        <a href="" class="title-mobile">Little Life</a>
      </div>
    </nav>
    <%else%>
 <nav>
      <div class="nav-fostrap">
        <ul>
          <li><%= link_to "Hi there!", '/'%></li>
          <li><%= link_to "Login", '/login' %></li>
          <li><%= link_to "Sign Up", '/signup' %></li>
           
          <li><a href="javascript:void(0)" >Navigation<span class="arrow-down"></span></a>
            <ul class="dropdown">
              <li><%= link_to "About", '/about' %></li>
              <li><%= link_to "See all users", '/test' %></li>
              <li><%= link_to "Help", '/help' %></li>
              <li><%= link_to "Home", '/' %></li>
            </ul>
          </li>
        </ul>
      </div>
      <div class="nav-bg-fostrap">
        <div class="navbar-fostrap"> <span></span> <span></span> <span></span> </div>
        <a href="" class="title-mobile">Little Life</a>
      </div>
    </nav>
    <% end %>

